#!/bin/bash
#SBATCH --time=15-0:0   # Especifica o tempo máximo de execução do job, dado no padrão dias-horas:minutos
for i in  5 7 10 12 15 17 20 22 25 27 30 33 35 38 40 42 45 48 50
do
	./santos < ../../GrafosCompletos/Conjunto2/3_OBJETIVOS/anticorrelated/"$i".anticorr1.in > Testes/GrafosCompletos/Conjunto2/anticorrelated/"$i".anticorr1.out
	./santos < ../../GrafosCompletos/Conjunto2/3_OBJETIVOS/anticorrelated/"$i".anticorr2.in > Testes/GrafosCompletos/Conjunto2/anticorrelated/"$i".anticorr2.out
	./santos < ../../GrafosCompletos/Conjunto2/3_OBJETIVOS/anticorrelated/"$i".anticorr3.in > Testes/GrafosCompletos/Conjunto2/anticorrelated/"$i".anticorr3.out
done



